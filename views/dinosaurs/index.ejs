<h2>Index of all dinos:</h2>

<ul>
    <% myDinos.forEach(dino => { %>
    <li><%= dino.name %> is a <%= dino.type %></li>
    <% }); %>
</ul>

<form method="GET" action="/dinosaurs">
    <label for="nameFilter">Filter by Name</label>
    <input id="nameFilter" type="text" name="nameFilter">
    <input type="submit">
</form>